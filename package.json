    "@types/node": "^20.5.1",
    "preact": "^10.17.1",
    "preact-render-to-string": "^6.2.1",
    "rollup": "^3.28.0",
    "rollup-plugin-serve": "^2.0.2",
    "sass": "^1.66.1",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6",
    "uglify-js": "^3.17.4"
  }
}
